<div class="page-header">
  <h1>New Terminology Source Upload</h1>
</div>
<%= form_for @package do |f| %>
    <%= f.label :user %><br />
    <%= f.text_field :user %>

    <%= f.fields_for :terminology_source_contents do |builder| %>
        <p>
          <%= builder.label :upload, 'Source Upload' %><br />
          <%= builder.file_field :upload, class: 'form-control' %>
        </p>
    <% end %>
    <p><%= f.submit 'Submit' %></p>
<% end %>
